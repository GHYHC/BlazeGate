		<None Include="..\..\README.md" Pack="true" PackagePath="" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\BlazeGate.Model\BlazeGate.Model.csproj" />
	</ItemGroup>

</Project>
